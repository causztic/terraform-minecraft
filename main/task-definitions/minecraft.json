[
  {
    "name": "minecraft",
    "image": "ruippeixotog/spigot",
    "cpu": 1,
    "memory": 2048,
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
          "awslogs-group": "mc",
          "awslogs-region": "ap-southeast-1",
          "awslogs-create-group": "true"
      }
    },
    "mountPoints": [
      {
          "containerPath": "/data",
          "sourceVolume": "minecraft"
      }
    ],
    "portMappings": [
      {
        "containerPort": 25565,
        "hostPort": 25565
      }
    ]
  }
]